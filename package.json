{
  "name": "blinkhash-server",
  "version": "0.0.1",
  "description": "An extremely efficient, highly scalable, all-in-one, easy to setup cryptocurrency mining pool",
  "keywords": [
    "stratum",
    "mining",
    "pool",
    "server",
    "poolserver",
    "bitcoin",
    "litecoin",
    "scrypt"
  ],
  "homepage": "https://github.com/blinkhash/blinkhash-server",
  "bugs": {
    "url": "https://github.com/blinkhash/blinkhash-server/issues"
  },
  "license": "GPL-2.0",
  "author": {
    "name": "Nick Sarris"
  },
  "jest": {
    "collectCoverage": true,
    "verbose": true
  },
  "scripts": {
    "start": "nodemon scripts/init.js",
    "clean": "eslint . --ext .js --ignore-pattern node_modules/",
    "clean:fix": "eslint . --ext .js --fix --ignore-pattern node_modules/",
    "test": "jest --coverage",
    "prod:start": "forever start -l blinkhash-log.txt --append scripts/init.js",
    "prod:stop": "forever stop scripts/init.js"
  },
  "contributors": [],
  "main": "scripts/init.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/blinkhash/blinkhash-server.git"
  },
  "dependencies": {
    "apicache": "^1.5.3",
    "async": "1.5.2",
    "bignum": "0.13.1",
    "blinkhash-stratum-pool": "0.0.1",
    "body-parser": "*",
    "colors": "*",
    "compression": "*",
    "cors": "^2.8.5",
    "dateformat": "1.0.12",
    "dot": "*",
    "express": "*",
    "extend": "*",
    "forever": "^3.0.4",
    "mysql": "*",
    "node-json-minify": "*",
    "node-watch": "0.5.9",
    "nodemon": "*",
    "nonce": "*",
    "redis": "^3.0.2",
    "redis-clustr": "^1.7.0",
    "request": "2.88.2"
  },
  "devDependencies": {
    "codecov": "^3.8.1",
    "eslint": "^7.23.0",
    "jest": "^26.6.3",
    "mitm": "^1.7.1",
    "nock": "^13.0.10"
  },
  "engines": {
    "node": ">=0.10"
  },
  "readmeFilename": "README.md"
}
